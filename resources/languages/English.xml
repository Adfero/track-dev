<?xml version="1.0" ?>
<language>
    <!-- The name of this language -->
    <name>English</name>
    <!-- The abbreviation for this language -->
    <short>en</short>
    <!-- The author of this language file. -->
    <author>creatorfromhell</author>
    <!-- The version of this language file. -->
    <version>1.0</version>
    <!-- The symbol for this language. Located in the theme's image folder. -->
    <symbol>us.gif</symbol>

    <!-- Everything that can be translated to different languages is located below. -->
    <!-- Words starting with % are shortcuts that are replaced by the script before processing. Please do not translate
         any of the shortcuts. -->
    <site>
        <title>%project - Trackr</title>
        <header>%list - %project</header>
        <welcome>welcome %user</welcome>
        <loginmessage>login or register</loginmessage>
        <login>login</login>
        <register>register</register>
        <email>email</email>
        <username>username</username>
        <password>password</password>
        <confirm>confirm</confirm>
        <pages>
            <overview>
                <navlink>Overview</navlink>
                <header>Overview</header>
                <calendar>
                    <navlink>Calendar</navlink>
                    <header>Calendar - %project</header>
                </calendar>
                <general>
                    <navlink>General</navlink>
                    <header>General - Overview</header>
                </general>
                <project>
                    <navlink>Project</navlink>
                    <header>Overview for %project</header>
                </project>
            </overview>
            <admin>
                <navlink>Admin</navlink>
                <header>Administration Panel</header>
                <groups>Groups</groups>
                <options>Options</options>
                <themes>Themes</themes>
                <users>Users</users>
            </admin>
            <projects>
                <navlink>Projects</navlink>
                <header>Project %project</header>
            </projects>
            <lists>
                <navlink>Lists</navlink>
                <header>Lists for %project</header>
            </lists>
        </pages>
        <tables>
            <name>Name</name>
            <assignee>Assignee</assignee>
            <created>Created</created>
            <creator>Creator</creator>
            <author>Author</author>
            <actions>Actions</actions>
            <overseer>Overseer</overseer>
            <permission>Permission</permission>
            <short>Short</short>
            <icon>Icon</icon>
            <admin>Admin</admin>
            <version>Version</version>
            <email>Email</email>
            <group>Group</group>
            <registered>Registered</registered>
            <yes>Yes</yes>
            <no>No</no>
            <none>none</none>
            <notask>There is nothing on the todo list.</notask>
            <nolist>There are currently no lists for this project.</nolist>
            <noproject>There are currently no projects.</noproject>
        </tables>
        <forms>
            <login>
                <invalidlogin>Invalid login credentials!</invalidlogin>
                <nopassword>No password entered!</nopassword>
                <nousername>No username entered!</nousername>
                <noemail>No email address entered!</noemail>
                <noconfirm>You must confirm your password!</noconfirm>
                <invalidpasswords>Passwords do not match!</invalidpasswords>
                <taken>Username already in use!</taken>
                <congrats>Thanks for registering! You may now login.</congrats>
            </login>
            <group>
                <nopermission>You must fill out the permission field!</nopermission>
                <noadmin>You must fill out the administrator field!</noadmin>
                <nopreset>You must fill out the preset field!</nopreset>
            </group>
            <project>
                <noid>Invalid id submitted!</noid>
                <noname>No name entered!</noname>
                <nomainlist>You must fill out the main list field!</nomainlist>
                <taken>A project with that name already exists!</taken>
                <noauthor>Invalid author entered!</noauthor>
                <nooverseer>Invalid overseer entered!</nooverseer>
                <nopublic>You must fill out the public field!</nopublic>
                <nomain>You must fill out the main field!</nomain>
            </project>
            <list>
                <created>The list %list has been created!</created>
                <taken>A list with that name already exists!</taken>
                <invalidproject>The project specified does not exist!</invalidproject>
                <noproject>You must enter a project for this list!</noproject>
                <nominimal>You must fill out the minimal field!</nominimal>
                <noguestview>You must fill out the guest view field!</noguestview>
                <noguestedit>You must fill out the guest edit field!</noguestedit>
                <noviewperm>You must fill out the view permission field!</noviewperm>
                <noeditperm>You must fill out the edit permission field!</noeditperm>
            </list>
            <task>
                <invalidlist>The list specified does not exist!</invalidlist>
                <invalidstatus>Invalid status entered!</invalidstatus>
                <notitle>You must fill out the title field!</notitle>
                <nodescription>You must fill out the description field!</nodescription>
                <noeditable>You must fill out the editable field!</noeditable>
            </task>
        </forms>
    </site>
    <email>
        <subject>Trackr Verification</subject>
        <verification>Welcome %user,\r\rYou, or someone using your email address, has completed registration for %url. You may complete your registration by clicking the following link: %activation\r\rIf this is an error you may ignore this email. \r\rSincerely, Website Administrator</verification>
    </email>
</language>